  </appSettings>
  <connectionStrings>
    <add name="SQLDEV" providerName="System.Data.SqlClient" connectionString="Data Source=PWANG;Initial Catalog=test;Integrated Security=true" />
    <add name="SQLQA" providerName="System.Data.SqlClient" connectionString="Data Source=SQLQA;Initial Catalog=iStage;Integrated Security=true" />
    <add name="SQLUAT" providerName="System.Data.SqlClient" connectionString="Data Source=SQLUAT;Initial Catalog=iStage;Integrated Security=true" />
    <add name="SQLPROD" providerName="System.Data.SqlClient" connectionString="Data Source=SQLPROD;Initial Catalog=iStage;Integrated Security=true" />
